{
  "name": "eofol3",
  "version": "0.3.0",
  "description": "Create a reactive app from template with zero configuration!",
  "scripts": {
    "start": "node ./bin/eofol-scripts.js start",
    "build": "node ./bin/eofol-scripts.js build",
    "clean": "node ./bin/eofol-scripts.js clean",
    "serve": "node ./bin/eofol-scripts.js serve",
    "compile-ts": "tsc -p ./tsconfig.json",
    "compile-after-ts": "node ./scripts/before-compile.js",
    "compile-static": "node ./scripts/compile.js",
    "compile-build": "npm run build",
    "compile-after-build": "node ./scripts/after-build.js",
    "precompile": "npm run compile-ts && npm run compile-after-ts && npm run compile-static",
    "postcompile": "npm run compile-build && npm run compile-after-build",
    "estart": "npm run precompile && npm run start",
    "eserve": "npm run precompile && npm run postcompile && npm run serve",
    "emid": "npm run precompile && npm run postcompile",
    "eclean": "node ./scripts/eclean.js",
    "prettify": "npx prettier . --write",
    "lint": "npx eslint ."
  },
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^16.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-class-properties": "^7.24.6",
    "@babel/plugin-transform-object-rest-spread": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@eofol/eofol-dev-utils": "^0.2.26",
    "@eslint/js": "^9.6.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "html-minifier-terser": "^7.2.0",
    "html-to-json-parser": "^2.0.1",
    "html-validator": "^6.0.1",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "3.3.2",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sharp": "^0.33.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.15.0",
    "uglify-js": "^3.18.0",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-dev-server": "^5.0.4"
  },
  "files": [
    "public",
    "src",
    ".babelrc",
    ".env",
    "eofol-app.d.ts",
    "README.md",
    "tsconfig.json"
  ],
  "author": "wurducius@gmail.com",
  "license": "MIT",
  "bugs": "https://github.com/wurducius/eofol/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wurducius/eofol.git",
    "directory": "packages/eofol-app"
  }
}
