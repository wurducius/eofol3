{
  "name": "eofol3",
  "version": "0.3.0",
  "description": "Create a reactive app from template with zero configuration!",
  "scripts": {
    "start": "node ./scripts/start.js",
    "start-partes": "npm run clean && npm run compile-ts && npm run compile-before && npm run compile-static && npm run compile-after && npm run serve",
    "build": "node ./scripts/build.js",
    "build-serve": "node ./scripts/build-serve.js",
    "serve": "node ./scripts/serve.js",
    "clean": "node ./scripts/clean.js",
    "compile-ts": "node ./scripts/compile-ts.js",
    "compile-before": "node ./scripts/before-compile.js",
    "compile-static": "node ./scripts/compile.js",
    "compile-after": "node ./scripts/after-compile.js",
    "reinstall": "node ./scripts/reinstall.js",
    "reinstall-force": "node ./scripts/reinstall.js -c",
    "analyze": "node ./scripts/analyze.js 1",
    "prettify": "npx prettier --config .prettierrc --write .",
    "check": "npx eslint -c eslint.config.mjs .",
    "check-fix": "npx eslint -c eslint.config.mjs --fix .",
    "lint": "npm run check-fix && npm run prettify",
    "test-smoke": "node ./scripts/test-smoke.js",
    "deploy": "npm run build -- --prod && call c:/code/deploy.bat && npm run test-smoke",
    "docker-build": "docker build -t eofol3-test ./docker-test",
    "docker-run": "docker run -p 3000:3000 eofol3-test",
    "docker-stop": "bash ./docker-test/docker-stop.sh"
  },
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "chalk": "2.4.2",
    "cross-spawn": "^7.0.3",
    "eofol-live-server": "^1.2.4",
    "eslint": "^9.8.0",
    "globals": "^15.8.0",
    "gzip-js": "^0.3.2",
    "html-minifier-terser": "^7.2.0",
    "html-to-json-parser": "^2.0.1",
    "less": "^4.2.0",
    "prettier": "3.3.3",
    "sharp": "^0.33.4",
    "typescript": "5.5.4",
    "typescript-eslint": "^7.17.0",
    "uglify-js": "^3.19.0",
    "watchpack": "^2.4.1"
  },
  "files": [
    "eofol",
    "static",
    "pages",
    "src",
    "scripts",
    ".env",
    "eofol-config.js",
    "eofol-app.d.ts",
    "README.md",
    "tsconfig.json"
  ],
  "author": "wurducius@gmail.com",
  "license": "MIT",
  "bugs": "https://github.com/wurducius/eofol3/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wurducius/eofol3.git"
  },
  "overrides": {
    "eslint": "^9.8.0"
  }
}
