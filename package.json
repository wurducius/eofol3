{
  "name": "eofol3",
  "version": "0.3.0",
  "description": "Create a reactive app from template with zero configuration!",
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "build-serve": "node ./scripts/build-serve.js",
    "serve": "node ./scripts/serve.js",
    "clean": "node ./scripts/clean.js",
    "compile-ts": "node ./scripts/compile-ts.js",
    "compile-before": "node ./scripts/before-compile.js",
    "compile-static": "node ./scripts/compile.js",
    "compile-after": "node ./scripts/after-compile.js",
    "prettify": "npx prettier --config .prettierrc --write .",
    "check": "npx eslint -c ./eslint.config.js . && npx eslint -c ./src/eslint.config.js ./src",
    "check-fix": "npx eslint -c ./eslint.config.js --fix . && npx eslint -c ./src/eslint.config.js --fix ./src",
    "lint": "npm run check-fix && npm run prettify"
  },
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.7.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "chalk": "2.4.2",
    "cross-spawn": "^7.0.3",
    "eslint": "8.56.0",
    "gzip-js": "^0.3.2",
    "html-minifier-terser": "^7.2.0",
    "html-to-json-parser": "^2.0.1",
    "html-validator": "^6.0.1",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "prettier": "3.3.2",
    "sharp": "^0.33.4",
    "typescript": "5.4.5",
    "uglify-js": "^3.18.0",
    "watchpack": "^2.4.1"
  },
  "files": [
    "eofol",
    "static",
    "pages",
    "src",
    "scripts",
    ".env",
    "eofol-config.js",
    "eofol-app.d.ts",
    "README.md",
    "tsconfig.json"
  ],
  "author": "wurducius@gmail.com",
  "license": "MIT",
  "bugs": "https://github.com/wurducius/eofol/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wurducius/eofol3.git"
  }
}
