{
  "name": "eofol3",
  "version": "0.3.0",
  "description": "Create a reactive app from template with zero configuration!",
  "scripts": {
    "start": "npm run clean && npm run precompile && npm run start",
    "eserve": "npm run clean && npm run precompile && npm run postcompile && npm run serve",
    "serve": "node ./bin/eofol-scripts.js serve",
    "build": "npm run clean && npm run precompile && npm run postcompile",
    "clean": "node scripts/clean.js",
    "compile-ts": "tsc -p ./tsconfig.json",
    "compile-after-ts": "node ./scripts/before-compile.js",
    "compile-static": "node ./scripts/compile.js",
    "compile-after-build": "node ./scripts/after-build.js",
    "precompile": "npm run compile-ts && npm run compile-after-ts",
    "postcompile": "npm run compile-static && npm run compile-after-build",
    "prettify": "npx prettier --config ./.prettierrc . --write",
    "check": "npx eslint -c ./eslint.config.js .",
    "check-fix": "npx eslint -c ./eslint.config.js --fix .",
    "lint": "npm run check-fix && npm run prettify"
  },
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-object-rest-spread": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eofol/eofol-dev-utils": "^0.2.26",
    "eslint": "^9.6.0",
    "gzip-js": "^0.3.2",
    "html-minifier-terser": "^7.2.0",
    "html-to-json-parser": "^2.0.1",
    "html-validator": "^6.0.1",
    "http-server": "^14.1.1",
    "prettier": "3.3.2",
    "sharp": "^0.33.4",
    "typescript": "5.4.5",
    "uglify-js": "^3.18.0"
  },
  "files": [
    "public",
    "src",
    ".env",
    "eofol-app.d.ts",
    "README.md",
    "tsconfig.json"
  ],
  "author": "wurducius@gmail.com",
  "license": "MIT",
  "bugs": "https://github.com/wurducius/eofol/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wurducius/eofol.git",
    "directory": "packages/eofol-app"
  }
}
